<window anim_hint="htranslate" name="window_line_series_colorful" tr_text="line_series_colorful">
    <chart_view name="chartview" x="6%" y="13%" w="80%" h="80%" disg:custom="container">
        <x_axis name="xfrom_series" h="50" axis_type="time" min="0" max="50" line="{lengthen:15}" disg:custom="container"/>
        <y_axis name="y" w="50" axis_type="value" min="0" max="140" data="[0,20,40,60,80,100,120,140]" line="{lengthen:15}" disg:custom="container"/>
        <line_series_colorful name="s1" capacity="1000" display_mode="cover" line="{smooth:true}" area="{show:true}" symbol="{show:true}" disg:custom="container"/>
        <tooltip disg:custom="container"/>
    </chart_view>
    <view x="right" y="0" w="101" h="100%" style="seperate"/>
    <view name="right_menu" x="right" y="0" w="100" h="100%" layout="r6 c1 m19 s18" style="function_view">
        <button name="new_graph" style="new"/>
        <button name="line" style="line_select"/>
        <button name="area" style="area_select"/>
        <button name="symbol" style="symbol_select"/>
        <button name="smooth" style="smooth_select"/>
        <button name="close" style="back"/>
    </view>
</window>
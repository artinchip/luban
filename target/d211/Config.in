config LUBAN_BOARD_D211_PER2_SPINOR
	bool "per2 spinor"
	depends on LUBAN_CHIP_D211

config LUBAN_BOARD_D211_PER2_SPINAND
	bool "per2 spinand"
	depends on LUBAN_CHIP_D211

config LUBAN_BOARD_D211_DEMO
	bool "demo"
	depends on LUBAN_CHIP_D211

config LUBAN_BOARD_D211_DEMO88_NAND
